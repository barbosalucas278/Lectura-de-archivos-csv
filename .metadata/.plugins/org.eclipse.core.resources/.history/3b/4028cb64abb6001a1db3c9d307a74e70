int articulo_printAllArticles (LinkedList* pList , int lenList){
	int ret = -1;
	int i;
	eArticulo* this;
	if(pList != NULL && lenList >= 0){
		for(i = 0; i <lenList; i++){
			this = ll_get(pList,i);
			 articulo_printArticle(this);
			 ret = 1;
		}
	}
	return ret;
}
