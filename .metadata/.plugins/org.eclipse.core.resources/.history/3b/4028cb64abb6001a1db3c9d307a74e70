int distribuidora_readFromFlie(LinkedList* pList){
	int ret = -1;
	FILE* file = openFileFromText();
	if(file != NULL){
		if(parser_articlesFromText(file,pList)){
			ret = 1;
		}
	}
	return ret;
}
